{
  "name": "realgrid",
  "author": {
    "name": "WooriTech",
    "url": "http://www.realgrid.com",
    "email": "support@realgrid.com"
  },
  "main": "./dist/main.js",
  "version": "2.4.1",
  "buildNo": "1357",
  "scripts": {
    "build": "tsc",
    "bundle": "tsc && rollup --config rollup.config.js",
    "dbundle": "tsc && rollup --config rollup.config.js  --environment BUILD:debug",
    "pbundle": "tsc && rollup --config rollup.config.js  --environment BUILD:prod",
    "deploy": "rollup --config rollup.config.js --environment BUILD:deploy && node compress.js",
    "prodeploy": "npm run pbundle && npm run deploy",
    "lib": "rollup --config rollup.config.js  --environment BUILD:lib",
    "ext": "tsc -p libtypeconfig.json && api-extractor run && node dts-replace.js",
    "lbundle": "npm run lib && npm run ext",
    "docs": "tsc -p docsconfig.json",
    "test": "mocha --require ts-node/register test/dom-mock.js test/**/*Test*.ts",
    "test2": "mocha --require ts-node/register test/dom-mock.js $1",
    "uitest": "cypress open"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.7.13",
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.7",
    "chai": "4.2.0",
    "commonjs": "0.0.1",
    "compressing": "1.5.1",
    "convert-csv-to-array": "1.0.3",
    "cypress": "3.4.1",
    "express": "4.17.1",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "6.1.4",
    "mocha-jsdom": "2.0.0",
    "path": "0.12.7",
    "pkginfo": "0.4.1",
    "reload": "3.0.4",
    "replace-in-file": "6.1.0",
    "rollup": "2.8.0",
    "rollup-plugin-banner": "0.2.0",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-copy": "3.3.0",
    "rollup-plugin-copy-assets": "2.0.1",
    "rollup-plugin-delete": "1.2.0",
    "rollup-plugin-modify": "3.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.3.0",
    "rollup-plugin-typescript": "1.0.1",
    "rollup-plugin-typescript2": "0.27.0",
    "rollup-plugin-uglify": "6.0.2",
    "terser": "4.6.10",
    "ts-loader": "7.0.1",
    "ts-node": "8.3.0",
    "tslib": "1.11.1",
    "typescript": "3.8.3",
    "xmldom": "0.1.27"
  },
  "dependencies": {
    "jszip": "^3.2.2"
  },
  "license": "license.txt"
}
